<?xml version="1.0" encoding="UTF-8"?>
<Export generator="Cache" version="25">
<Class name="Fileserver.Download">
<Description>
One download of a file. </Description>
<Final>1</Final>
<Super>%Persistent</Super>
<TimeCreated>63879,84547</TimeCreated>
<ServerOnly>1</ServerOnly>

<Index name="DownloadIDIndex">
<Internal>1</Internal>
<IdKey>1</IdKey>
<PrimaryKey>1</PrimaryKey>
<Properties>DownloadID</Properties>
</Index>

<Property name="DownloadID">
<Description>
Random letter combination. NOT a private key.</Description>
<Type>%String</Type>
<Internal>1</Internal>
<InitialExpression>##class(%xsd.hexBinary).LogicalToXSD($system.Encryption.GenCryptRand(50,$$$YES))</InitialExpression>
<Private>1</Private>
<SqlColumnNumber>6</SqlColumnNumber>
<ReadOnly>1</ReadOnly>
<Parameter name="MAXLEN" value="100"/>
<Parameter name="MINLEN" value="100"/>
</Property>

<Property name="File">
<Description>
File to download.</Description>
<Type>Fileserver.File</Type>
<Cardinality>one</Cardinality>
<Inverse>Downloads</Inverse>
<Relationship>1</Relationship>
<Required>1</Required>
<SqlColumnNumber>2</SqlColumnNumber>
<OnDelete>cascade</OnDelete>
</Property>

<Property name="DateCreated">
<Description><![CDATA[
Creation date of download  in <i>YYYY-MM-DD hh:mm:ss</i> format.]]></Description>
<Type>%TimeStamp</Type>
<InitialExpression>$ZDATETIME($NOW(),3,1,6)</InitialExpression>
<SqlColumnNumber>3</SqlColumnNumber>
<ReadOnly>1</ReadOnly>
</Property>

<Property name="DownloadCount">
<Description>
Number of filetransfers occured for this download.</Description>
<Type>%Integer</Type>
<InitialExpression>0</InitialExpression>
<Required>1</Required>
<SqlColumnNumber>4</SqlColumnNumber>
<Parameter name="MINVAL" value="0"/>
</Property>

<Property name="IP">
<Description>
IP of a client initiated creation of download.</Description>
<Type>%NetworkAddress</Type>
<InitialExpression>%request.GetCgiEnv("REMOTE_ADDR")</InitialExpression>
<Required>1</Required>
<SqlColumnNumber>5</SqlColumnNumber>
<ReadOnly>1</ReadOnly>
</Property>

<Storage name="Default">
<Type>%Library.CacheStorage</Type>
<DataLocation>^Fileserver.DownloadD</DataLocation>
<DefaultData>DownloadDefaultData</DefaultData>
<IdLocation>^Fileserver.DownloadD</IdLocation>
<IndexLocation>^Fileserver.DownloadI</IndexLocation>
<StreamLocation>^Fileserver.DownloadS</StreamLocation>
<ExtentSize>100000</ExtentSize>
<Data name="DownloadDefaultData">
<Structure>listnode</Structure>
<Subscript/>
<Value name="1">
<Value>File</Value>
</Value>
<Value name="2">
<Value>DateCreated</Value>
</Value>
<Value name="3">
<Value>DownloadCount</Value>
</Value>
<Value name="4">
<Value>IP</Value>
</Value>
</Data>
</Storage>
</Class>
</Export>
